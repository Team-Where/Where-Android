<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/meetTabFragment">

    <fragment
        android:id="@+id/meetTabFragment"
        android:name="com.sooum.where_android.view.main.myMeetDetail.fragment.MyMeetTabFragment"
        android:label="MeetTabFragment">
        <action
            android:id="@+id/action_tabFragment_to_ScheduleFragment"
            app:destination="@id/scheduleFragment"
            app:enterAnim="@anim/slide_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/slide_out" />

        <action
            android:id="@+id/action_tabFragment_to_InviteFriendFragment"
            app:destination="@id/inviteFriendFragment"
            app:enterAnim="@anim/slide_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/slide_out" />

        <action
            android:id="@+id/action_tabFragment_to_PlaceDetailFragment"
            app:destination="@id/placeDetailFragment"
            app:enterAnim="@anim/slide_in"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/slide_out" />
    </fragment>

    <fragment
        android:id="@+id/scheduleFragment"
        android:name="com.sooum.where_android.view.main.myMeetDetail.modal.schedule.ScheduleFragment"
        android:label="ScheduleFragment" />

    <fragment
        android:id="@+id/inviteFriendFragment"
        android:name="com.sooum.where_android.view.main.myMeetDetail.modal.invite.InviteFriendFragment"
        android:label="InviteFriendFragment" />

    <fragment
        android:id="@+id/placeDetailFragment"
        android:name="com.sooum.where_android.view.main.myMeetDetail.fragment.MyMeetPlaceDetailFragment"
        android:label="PlaceDetailFragment" />

</navigation>